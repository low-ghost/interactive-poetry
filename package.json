{
  "name": "interactive-poetry",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "homepage": "https://low-ghost.github.io/interactive-poetry",
  "scripts": {
    "dev": "vite",
    "build": "npm run slides && tsc -b && vite build",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "preview": "vite preview",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "slides": "marp slides.md --html --output-dir public/slides && cp -r images public/slides/"
  },
  "dependencies": {
    "@p5-wrapper/react": "^4.4.2",
    "@types/ramda": "^0.30.2",
    "lucide-react": "^0.503.0",
    "mathjs": "^14.4.0",
    "p5": "^2.0.0",
    "ramda": "^0.30.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "^6.30.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.22.0",
    "@marp-team/marp-cli": "^3.4.0",
    "@rollup/plugin-inject": "^5.0.5",
    "@types/p5": "^1.7.6",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitejs/plugin-react-swc": "^3.8.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.22.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "gh-pages": "^6.3.0",
    "globals": "^16.0.0",
    "postcss": "^8.5.3",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.26.1",
    "vite": "^6.3.1"
  }
}
